  language = {english},
  publication_stage = {Final},
  source = {Scopus},
  file = {/Users/yektaamirkhalili/Zotero/storage/QGBPCI6J/Mensah_Khan_2024_Unified Theory of Acceptance and Use of Technology (UTAUT) Model.pdf}
}

