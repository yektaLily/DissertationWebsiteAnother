@article{mufarihFactorsInfluencingCustomers2020a,
  type = {Article},
  title = {Factors {{Influencing Customers}} to {{Use Digital Banking Application}} in {{Yogyakarta}}, {{Indonesia}}},
  author = {MUFARIH, Muhammad and JAYADI, Riyanto and SUGandI, Yovin},
  year = {2020},
  journal = {Journal of Asian Finance, Economics and Business},
