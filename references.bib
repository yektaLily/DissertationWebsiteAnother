@article{yinPredictorsCustomersContinuance2022,
  type = {Article},
  title = {Predictors of {{Customers}}' {{Continuance Intention}} of {{Mobile Banking}} from the {{Perspective}} of the {{Interactivity Theory}}},
  author = {Yin, Lan-Xiang and Lin, Hsien-Cheng},
  year = {2022},
  journal = {Economic Research-Ekonomska Istrazivanja},
  volume = {35},
  number = {1},
  pages = {6820--6849},
  publisher = {{Taylor and Francis Ltd.}},
  issn = {1331677X},
  doi = {10.1080/1331677X.2022.2053782},
  abbrev_source_title = {Economic Res. Ekon. Istraz.},
  abstract = {In order to test the key factors affecting users' continuance intention of mobile Banking Apps, this study combined the Technology acceptance model (TAM) with the perceptual sinteraction model, and further added human-system interaction and perceived privacy security that was not explored in these models before. Data were collected from 349 users who had experience with mobile Banking to test the model and were analyzed with a structural equation model. The present study showed that various interactive characteristics of mobile Banking (human-human interaction, human-information interaction, human-system interaction) had significant positive impacts on users' perceptions of using mobile Banking (perceived usefulness, perceived ease of use, perceived privacy security). Users' perceptions of using mobile Banking had significantly positive impacts on users' satisfaction with mobile Banking, and user satisfaction had a significant impact on users' continuance intention. Users' income level had a significantly negative impact on their continuance intention. In order to further enhance users' continuance intention for mobile Banking, designers and managers of mobile Banking are suggested to pay special attention to the interactive characteristics of mobile Banking and the impact on users' perceptions of mobile Banking, so as to improve customer satisfaction and continuous intention. {\copyright} 2022 The Author(s). Published by Informa UK Limited, trading as Taylor \& Francis Group.},
  affiliations = {Business College, Taizhou University, Taizhou City, China},
  author_keywords = {A13; continuance intention; D12; D91; G41; L86; Mobile Banking app; perceived interactivity; satisfaction; technology acceptance model (TAM)},
  correspondence_address = {L.-X. Yin; Business College, Taizhou University, Taizhou City, China; email: zmjxylx@tzc.edu.cn},
  language = {english},
  publication_stage = {Final},
  source = {Scopus},
  keywords = {Banking,capital flow,computer simulation,economic development,innovation,investment,mobile phone}
}

