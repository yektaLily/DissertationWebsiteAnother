@article{merkRoleAffectAttitude2017,
  title = {The {{Role}} of {{Affect}} in {{Attitude Formation}} toward {{New Technologies}}: {{The Case}} of {{Stratospheric Aerosol Injection}}},
  shorttitle = {The {{Role}} of {{Affect}} in {{Attitude Formation}} toward {{New Technologies}}},
  author = {Merk, Christine and P{\"o}nitzsch, Gert},
  year = {2017},
  journal = {Risk Analysis},
  volume = {37},
  number = {12},
  pages = {2289--2304},
  issn = {0272-4332, 1539-6924},
  doi = {10.1111/risa.12780},
  urldate = {2024-12-04},
  abstract = {Abstract This article analyzes determinants of technology acceptance and their interdependence. It highlights the role of affect in attitude formation toward new technologies and examines how it mediates the influence of stable psychological variables on the technology's acceptability. Based on theory and previous empirical evidence, we develop an analytical framework of attitude formation. We test this framework using survey data on attitudes toward stratospheric aerosol injection (SAI), a technology that could be used to counteract global warming. We show that affect is more important than risk and benefit perception in forming judgment about SAI. Negative and positive affect directly alter the perception of risks and benefits of SAI and its acceptability. Furthermore, affect is an important mediator between stable psychological variables---such as trust in governmental institutions, values, and attitudes---and acceptability. A person's affective response is thus guided by her general attitudes and values.},
  language = {english},
  file = {/Users/yektaamirkhalili/Zotero/storage/G6VDNWGX/Merk_Pönitzsch_2017_The Role of Affect in Attitude Formation toward New Technologies.pdf}
}

